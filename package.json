{
  "name": "@psychobolt/react-rollup-boilerplate",
  "description": "A boilerplate for building React libraries",
  "type": "module",
  "module": "./dist/index.prod.js",
  "main": "./dist/index.prod.cjs",
  "exports": {
    "development": {
      "module": "./dist/index.dev.js",
      "import": "./dist/index.dev.cjs.mjs",
      "require": "./dist/index.dev.cjs"
    },
    "module": "./dist/index.prod.js",
    "import": "./dist/index.prod.cjs.mjs",
    "require": "./dist/index.prod.cjs",
    "default": "./dist/index.prod.js"
  },
  "repository": "https://github.com/psychobolt/react-rollup-boilerplate",
  "author": "psychobolt",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "flow-deps-link": "yarn symlink-dir packages/flow-deps/node_modules flow-deps-modules",
    "g:flow-typed-update": "cd $INIT_CWD && flow-typed update --libdefDir shared/flow-typed -i peer -s --skipFlowRestart -p $PROJECT_CWD",
    "flow-typed-update": "yarn workspaces foreach -A --no-private run g:flow-typed-update",
    "flow-typed-install": "rimraf shared/flow-typed/npm && yarn flow-deps-link && yarn flow-typed-update",
    "flow-typed-create-stub": "flow-typed create-stub --libdefDir shared/flow-typed/",
    "build:dev": "cross-env BABEL_ENV=development rollup -c rollup.config.dev.mjs",
    "build:prod": "cross-env rollup -c rollup.config.prod.mjs",
    "clean": "rimraf \"dist/index?(-*).*.?(c|m)js?(.flow)\" && rimraf \"packages/*/dist/index?(-*).*.?(c|m)js?(.flow)\"",
    "build": "yarn clean && yarn build:dev && yarn build:prod",
    "watch": "yarn build:dev --w",
    "start": "cross-env BABEL_ENV=development concurrently 'npm:watch' 'npm:dev-storybook'",
    "start-static": "yarn build && yarn build-storybook && yarn serve-storybook",
    "test": "cross-env BABEL_ENV=test node --experimental-vm-modules $(yarn bin jest) --passWithNoTests",
    "lint": "concurrently 'npm:lint:css' 'npm:lint:js'",
    "lint:css": "stylelint \"./(packages|stories|src)/**/*.js\"",
    "lint:js": "eslint \"**/*.?(c|m)js\"",
    "dev-storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "serve-storybook": "http-server storybook-static",
    "bootstrap": "node bootstrap.js",
    "prepack": "yarn build",
    "dist": "yarn workspaces foreach -A --no-private npm publish --tolerate-republish --access public"
  },
  "devDependencies": {
    "@babel/cli": "7.28.6",
    "@babel/core": "7.28.6",
    "@babel/eslint-parser": "7.28.6",
    "@babel/helper-compilation-targets": "7.28.6",
    "@babel/node": "7.28.6",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.28.6",
    "@babel/plugin-proposal-do-expressions": "7.28.6",
    "@babel/plugin-proposal-export-default-from": "7.27.1",
    "@babel/plugin-proposal-export-namespace-from": "7.18.9",
    "@babel/plugin-proposal-json-strings": "7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-numeric-separator": "7.18.6",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/plugin-proposal-private-methods": "7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@babel/plugin-proposal-throw-expressions": "7.27.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-flow": "7.28.6",
    "@babel/plugin-transform-react-jsx": "7.28.6",
    "@babel/preset-env": "7.28.6",
    "@babel/preset-flow": "7.27.1",
    "@babel/preset-react": "7.28.5",
    "@babel/template": "7.28.6",
    "@eslint/eslintrc": "3.3.3",
    "@mdx-js/react": "3.1.1",
    "@psychobolt/default-export": "workspace:^",
    "@rollup/plugin-alias": "5.1.1",
