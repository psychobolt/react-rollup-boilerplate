    "rollup-plugin-terser": "7.0.2",
    "sass": "1.97.3",
    "sass-loader": "16.0.6",
    "slash": "5.1.0",
    "source-map-loader": "5.0.0",
    "storybook": "7.6.21",
    "style-loader": "4.0.0",
    "styled-components": "6.1.19",
    "stylelint": "14.16.1",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "https://github.com/psychobolt/stylelint-processor-styled-components",
    "symlink-dir": "6.0.5",
    "webpack": "5.103.0",
    "webpack-merge": "6.0.1",
    "yargs": "17.7.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.0"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "packageManager": "yarn@4.5.3",
  "resolutions": {
    "@storybook/core-webpack@npm:7.6.17": "patch:@storybook/core-webpack@npm%3A7.6.17#~/.yarn/patches/@storybook-core-webpack-npm-7.6.17-4f0f3e4ec4.patch"
  }
}
