    "cash-true": "0.0.2",
    "concurrently": "9.2.1",
    "cross-env": "7.0.3",
    "css-loader": "6.11.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-react-app": "7.0.1",
    "eslint-import-resolver-alias": "1.1.2",
